{
  "name": "tivents/tivents-products-feed",
  "type": "plugin",
  "license": "MIT",
  "homepage": "https://github.com/tivents/tivents-products-feed",
  "description": "Plugin for getting products from TIVENTS",
  "keywords": ["code coverage", "codecov", "coverage reports"],
  "version": "1.6.0",
  "require": {
    "php": ">=8",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "wp-coding-standards/wpcs": "^3"
  },
  "autoload": {
    "psr-0": {
      "TiventsShopConnector": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
